{% extends 'base.html.twig' %}

{% block title %}HistoryLog{% endblock %}

{% block body %}
    <h1>HistoryLog</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ history_log.id }}</td>
            </tr>
            <tr>
                <th>Export_name</th>
                <td>{{ history_log.exportName }}</td>
            </tr>
            <tr>
                <th>Export_date</th>
                <td>{{ history_log.exportDate ? history_log.exportDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>User_name</th>
                <td>{{ history_log.userName }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ history_log.location }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_history_log_index') }}">back to list</a>

    <a href="{{ path('app_history_log_edit', {'id': history_log.id}) }}">edit</a>

    {{ include('history_log/_delete_form.html.twig') }}
{% endblock %}
